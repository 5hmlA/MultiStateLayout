<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiStateLayout">
        <attr name="error" format="reference"/>
        <attr name="loading" format="reference"/>
        <attr name="empty" format="reference"/>
        <attr name="state" format="enum">
            <enum name="loading" value="0"/>
            <enum name="error" value="1"/>
            <enum name="empty" value="2"/>
            <enum name="except" value="3"/>
        </attr>
    </declare-styleable>
</resources>